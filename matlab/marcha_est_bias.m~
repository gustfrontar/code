hold on
plot(squeeze(pp_datos_n(7,3,1,t_datos_n-30:t_datos_n)),'-.r*','DisplayName','Pronostico a 48hs','LineWidth',2)
plot(squeeze(pp_datos_n(7,5,1,t_datos_n-30:t_datos_n)),'--mo','DisplayName','Pronostico a 72hs','LineWidth',2)
legend('Pronostico a 24hs','Pronostico a 48hs','Pronostico a 72hs',0)



